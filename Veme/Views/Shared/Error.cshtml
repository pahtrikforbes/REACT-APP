@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "Error";
}
@*<h1 class="text-danger">Error.</h1>
    <h2 class="text-danger">An error occurred while processing your request.</h2>*@
<style>
    .error-body h1 {
        font-size: 210px;
        font-weight: 900;
        line-height: 210px;

    }
    html body .text-muted {
        color: #90a4ae;
    }
</style>
<section id="wrapper" class="error-page">
    <div class="error-box">
        <div class="error-body text-center">
            <h1 class="text-danger">Oops!</h1>
            <br />
            <h3 class="text-uppercase">Page Not Found!</h3>
            <p class="text-muted m-t-30 m-b-30">Something went wrong with you request</p>
            @*<a href="Account/Login.cshtml" class="btn btn-info btn-rounded waves-effect waves-light m-b-40">Back to home</a>*@
        </div>
        <!--<footer class="footer text-center">2017 © Monster Admin.</footer>-->
    </div>
</section>

